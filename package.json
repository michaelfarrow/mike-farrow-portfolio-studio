  "license": "UNLICENSED",
  "scripts": {
    "dev": "concurrently --names 'studio,watch' -c 'bgGreen.bold,bgMagenta.bold' \"sanity dev\" \"npm run watch\"",
    "start": "sanity start",
    "build": "sanity build",
