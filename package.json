    "sanity": "^3.69.0",
    "sanity-plugin-iframe-pane": "^3.1.6",
    "styled-components": "^6.1.8",
    "title-case": "^4.3.2"
  },
  "devDependencies": {
    "@sanity/eslint-config-studio": "^5.0.1",
    "@types/react": "^18.0.25",
    "chokidar-cli": "^3.0.0",
    "concurrently": "^9.1.2",
    "eslint": "^9.9.0",
    "prettier": "^3.0.2",
    "type-fest": "^4.32.0",
    "typescript": "^5.1.6",
    "vite-tsconfig-paths": "^5.1.4"
